import { print createInput } from "../node.json"

function main() void {
	print("in main")
	createInput(onInput)
}

function onInput(line:string) void {
	if (line == "") {
		print("nothing")
	}
	if (line == "ping") {
		print("pong")
	}
	if (line == "no cats") {
		print("amount of cats = 0")
	}
	if (line == "cat") {
		print("amount of cats = 1")
	}
	if (line == "cats") {
		print("amount of cats > 1")
	}
	if (line == "hi") {
		print("hello")
	}
	if (line == "hello") {
		print("hi")
	}
	/*switch {
		(line == "hi") {

		}
		(line == "hello") {

		}
		(true) {

		}
	}*/
}