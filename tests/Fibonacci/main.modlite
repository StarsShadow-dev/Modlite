import { print } from "StandardLibrary"

// the Fibonacci sequence without recursion!

var beforeLast string
var last string
var string string

function main() void {
	beforeLast = 0
	last = 1
	string = "0 1"

@duplicate<30>
	fibonacci()
@end

	print(string)
}

function fibonacci() void {
	var oldLast string

	oldLast = last

	last = beforeLast + last
	beforeLast = oldLast

	string = string .. " " .. last
}