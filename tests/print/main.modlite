import { printLine } from "StandardLibrary"

function main(): Void {
	print("in main")

	print(true)

	print(false)

	print5x("print5x")
}

function print5x(msg: String): Void {
@duplicate<5>
	print(msg)
@end
}

@macros

function print(value: Any): Void {

@if_macro_type<value Bool>
	if (value! Bool) {
		printLine("[Bool] true")
	} else {
		printLine("[Bool] false")
	}
@end

@if_macro_type<value String>
	printLine(value! String)
@end

}

@end